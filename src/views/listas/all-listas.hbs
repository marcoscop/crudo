<div class="row">
  <div class="col-md-8">
    <h3>LISTAS QUE PARTICIPARAN DE LAS ELECCIONES</h3>
  </div>
  <div class="col-md-4">
    <a href="/listas/add"> <button class="btn btn-primary">Agregar Nueva Lista</button> </a>
  </div>
</div>
<hr />
<div class="row mt-5">
  {{#each listas}}
    <div class="col-md-3 mt-3">
      <a href="/listas/editar/{{this.id}}" class="text-white">
        <div class="card bg-success text-center">
          <div class="card-header">
            <strong>{{this.nombre_lista}}</strong>
            
          </div>
          <div class="card-body">            
            <img src="/imgListas/{{this.boleta_img}}" class="img-fluid" alt="" >
          </div>
        </div></a>
    </div>
  {{/each}}
</div>