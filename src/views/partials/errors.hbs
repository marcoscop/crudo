{{#if error}}
  <div class="alert alert-danger alert-dismissible fade show" role="alert">
    <strong>ERROR CRITICO!</strong>
    {{error}}
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
{{/if}}
{{#each errorMsg}}
  <div class="alert alert-danger alert-dismissible fade show" role="alert">
    <strong>{{msg}}</strong>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    >

    </button>
  </div>
{{/each}}

{{#each warningMsg}}
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>ATENCION!</strong>
    {{msg}}
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    >

    </button>
  </div>
{{/each}}