<div class="row">
  <div class="col-md-6">
    <h1 class="text-primary">RESULTADOS</h1>
  </div>
  <div class="col-md-6">
      <div class="d-grid gap-2 d-md-flex justify-content-md-end">
        <a href="/"> <button class="btn btn-primary">INICIO</button> </a>
      </div>
  </div>
</div>
        <h3>RESULTADO DE LAS VOTACIONES</h3>                
    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">NOMBRE LISTA</th>
      <th scope="col">CANTIDAD VOTOS</th>      
    </tr>
  </thead>
  <tbody>
      {{#each listaResultados}}
    <tr        
      <th scope="row"></th>
      <td>{{@index}}</td>
      <td>{{this.nombre_lista}}</td>
      <td>{{this.cantidadVotos}}</td>      
    </tr>   
    {{/each}}
  </tbody>
</table>
